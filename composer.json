{
    "name": "humble23/cartola-fc-api",
    "description": "PHP API client for cartola fc",
    "keywords": [
        "humble23",
        "cartola-fc-api"
    ],
    "homepage": "https://github.com/humble23/cartola-fc-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Edilson Fernandes",
            "email": "edilsonfernandes312@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Humble23\\CartolaFcClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Humble23\\CartolaFcClientTests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
